{
  "env": {
    "jest": true,
    "jest/globals": true
  },
  "extends": ["google", "plugin:jest/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "strict": 2,
    "switch-colon-spacing": 0
  }
}